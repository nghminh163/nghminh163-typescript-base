{
  "compilerOptions": {
    "target": "ES2015",
    "module": "commonjs",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@config": [
        "./config/config"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@routes/*": [
        "src/routes/*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}
